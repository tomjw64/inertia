{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "outDir": "./dist/",
    "sourceMap": true,
    "module": "es2020",
    "target": "es6",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  }
}
